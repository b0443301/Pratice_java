
public class main {
	public static void main(String args[]) {
		// int[][] pervoteobtain = new int[4][3];
		int sum[] = new int[4];
		int vote = 0;
		int[][] pervoteobtain = { { 1, 2, 3 }, { 6, 7, 8 }, { 9, 10, 11 }, { 12, 13, 14 } };
		// int ans[] = new int[4];
		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 3; j++) {
				sum[i] += pervoteobtain[i][j];
				//sum[i] += sum[i];
			}
			System.out.println("第" + (i + 1) + "位明星得票數" + sum[i]);

		}

		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 3; j++) {
			vote += sum[i];
			vote=sum[i]/vote;
		}
		
		System.out.println("第" + (i + 1) + "位明星得票率" +vote);
}}
}
