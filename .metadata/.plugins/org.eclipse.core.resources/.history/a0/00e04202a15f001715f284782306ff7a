
public class main {
	public static void main(String args[]) {
		int i = 1;
		while (i <= 3) {
			int j = 1;// ²MªÅ
		
		while (j <= 9) {
				System.out.print((i * 3)-2 + "*" + j + "=" + ((i * 3)-2 * j) + "\t");
				System.out.print((i * 3)-1 + "*" + j + "=" + ((i * 3)-1 * j) + "\t");
				System.out.print((i * 3) + "*" + j + "=" + ((i * 3) * j) + "\t");
				j++;
				
			}
			
				System.out.print("\n");
			
			System.out.print("\n");
			i++;

		}

	}
}