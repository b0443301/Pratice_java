
public class main {
	public static int GreComDiv(int num1, int num2) {
		int i = 1;
		 int temp=0;
		for (; i<num1; i++) {
				if (num1 % i == 0 && num2 % i == 0)
					  temp = i;
					//System.out.println(i);
					
					//continue;
			}
		return temp;
	}

	public static void main(String args[]) {
		int num1 = 15;
		int num2 = 20;
		int c1 = GreComDiv(num1, num2);
		System.out.println("num1和num2公因式  " + c1);

	}

}
