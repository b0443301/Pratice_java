public class main{
	 int Array1(int array[],int data) {
		 //int array1[]={0,11,22,33,44,55,66};
		 
		for(int i=0;i<array.length;i++){
			if(data==array[i]){
				return i;
			}else{
				return -1;
			}
			final int main(String args[]){
