
public class Student {
	private int id;
	private String name;
	private int score;

	public void setName(String name) {
		this.name = name;
}

	public String getname(){
		return name;
	}
	public void setid(int id) {
		this.id = id;
}
	public int getid(){
		return id;
	}
	public void setscore(int score) {
		this.score = score;
}
	public int getscore(){
		return score;
	}
	
	
	
	
	
}
