public class main {
	public static void main(String args[]) {
		// power(1, 0.1, 10);
		divide(1, 5);
	}

	static void power(double num, double x, int count) {
		if (count > 0) {
			num = x * num;
			System.out.println(num + "\t" + x + "\t" + count);
			count--;
			power(num, x, count);
		}
	}

	static void divide(int sum, int count) {
		if (count > 0) {
			int temp = 2;
			
			sum++;
			temp=sum+1;
			temp = sum* temp;
			
			 
			
			System.out.println( temp + "\t" + sum + "\t" + count);
			count--;
			divide(sum, count);
		}
	}

}

