import java.util.Scanner;

public class main {
	class SemesterScore {
		String name = "";
		String id = "";
		int middleScore = 0;
		int midtermScore = 0;
		int ususalScore = 0;
		int semester = (int) ((middleScore * 0.3) + (midtermScore * 0.3) + (ususalScore * 0.4));

	}

	public static void main(String args[]) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("請輸入3筆學生資料");
		String semesterScore=new String();
		SemesterScore semesterScore=new SemesterScore();
		while (scanner.hasNextLine()) {
			semesterScore=scanner.nextLine();
		

	}

