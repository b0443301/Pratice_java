
public class main {
	public static void main(String args[]) {
		// int[][] pervoteobtain = new int[4][3];
		int sum[] = new int[4];
		int[][] pervoteobtain = { { 56, 46, 12 }, { 15, 14, 16 }, { 15, 14, 16 }, { 15, 14, 16 } };
		int ans[] = new int[4];
		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 3; j++) {
				sum[i] += sum[i];
				ans[i] = sum[i] / 4;

			}
			System.out.println("得票數" + sum[i] + "得票率" + ans[i] + " ");

		}

	}
}
