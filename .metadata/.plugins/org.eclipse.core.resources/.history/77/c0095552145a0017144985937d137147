import java.util.Scanner;

public class Student {
	public class Data {
		String id;
		String name;
		int score;
		public Data(){
			
		}

	}

	public static void main(String args[]) {
		Scanner scanner = new Scanner(System.in);
		Student.Data data=new Student.Data();
		String id;
		String name;
		String score;

		id = scanner.nextLine();
		name = scanner.nextLine();
		score = scanner.nextLine();

		System.out.println("請輸入學生Peter姓名" + name);
		System.out.println("請輸入學生Peter學號" + id);
		System.out.println("請輸入學生Peter分數" + score);
		
		System.out.println("請輸入學生Mary姓名" + name);
		System.out.println("請輸入學生Mary學號" + id);
		System.out.println("請輸入學生Mary分數" + score);
		
		System.out.println("Peter的姓名" + name);
		System.out.println("Peter的學號" + id);
		System.out.println("Peter的分數" + score);

		System.out.println("Mary的姓名" + name);
		System.out.println("Mary的學號" + id);
		System.out.println("Mary的分數" + score);
	}

}
