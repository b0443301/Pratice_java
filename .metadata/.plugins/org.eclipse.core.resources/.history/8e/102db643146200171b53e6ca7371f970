
public class main {
	public static void main(String arg[]) {
		char array1[] = { 'a', 'b', 'c' };
		char array2[] = { 'x', 'y', 'z' };
		for (int i = 0; i <3 ; i++) {
			 if(array1[i] != 'a'&&array2[i]!='x' )
				 continue;
			for (int j = 0; j < 3; j++) {
				for (int k = 0; k< 3; k++) {
				if ( array1[2] != array2[0] && array1[2] != array2[2]) {
					continue;
				}
			}
		}
		for (int i = 0; i < 3; i++) {
			System.out.println(array1[i]-'a'+array2[i]-'a');
		}
	}
}
