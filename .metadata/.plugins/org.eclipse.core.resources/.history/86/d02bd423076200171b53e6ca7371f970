
public class main {
	public static void main(String arg[]) {
		char array1[] = { 'a', 'b', 'c' };
		char array2[] = { 'x', 'y', 'z' };
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 3; j++) {
				if (array1[0] != array2[0] && array1[3] != array2[1] && array1[3] != array2[3]) {
					continue;
				}
			}
			System.out.println(array1[i]+array2[i]);
		}
	}
}
